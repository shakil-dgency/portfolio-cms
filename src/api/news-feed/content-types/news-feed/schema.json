{
  "kind": "collectionType",
  "collectionName": "news_feeds",
  "info": {
    "singularName": "news-feed",
    "pluralName": "news-feeds",
    "displayName": "NewsFeed",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "feed_title": {
      "type": "string",
      "required": true
    },
    "feed_description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "required": false,
      "customField": "plugin::ckeditor.CKEditor"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "video_url": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "feed_title",
      "required": true
    },
    "like_count": {
      "type": "string",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
